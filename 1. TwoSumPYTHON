class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        res = []

        for i in range(len(nums)):
            diff = target - nums[i]
            for j in range(i+1, len(nums)):
                if diff == nums[j]:
                    res.append(i)
                    res.append(j)
        return res
***************************************************************************
class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        nums_dict = {}

        for i, num in enumerate(nums):
            diff = target - num
            if diff in nums_dict:
                return [i, nums_dict[diff]]
            nums_dict[num] = i
        return []
