class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        st = []
        for ch in s:
            if ch == "(" or ch == "{" or ch == "[":
                st.append(ch)
            elif st and ((ch == ")" and st[-1] == "(" ) or \
                (ch == "}" and st[-1] == "{") or \
                (ch == "]" and st[-1] == "[")):
                st.pop()
            else:
                return False         
        return not st
